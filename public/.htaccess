<IfModule mod_rewrite.c>
    Options +FollowSymLinks
    Options -Indexes
    RewriteEngine On
</IfModule>

<IfModule mod_rewrite.c>
    RewriteBase /
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
#    RewriteRule ^([A-Z][a-z]+)$ index.php?cmd=$1 [NC,L,QSA]
    RewriteRule ^(.*)$ index.php/$1 [NC,L,QSA]
</IfModule>
